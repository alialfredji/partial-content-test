{
  "name": "starterapp-v2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "----- // CRA DEFAULT SCRIPTS // -----": "",
    "start": "yarn clean && yarn build && cross-env NODE_ENV=production node index",
    "build": "yarn build:app && yarn build:node",
    "analyze": "npx cra-bundle-analyzer",
    "----- // START SCRIPTS // -----": "",
    "start:api": "yarn build:node && cross-env NODE_ENV=production node index",
    "start:app": "react-scripts start",
    "start:dev": "concurrently \"yarn start:dev:api\" \"yarn start:app\"",
    "start:dev:api": "cross-env NODE_ENV=development nodemon --watch ssr -e js --watch node_build/app index.js",
    "----- // BUILD SCRIPTS // -----": "",
    "build:app": "react-scripts build && mkdir -p node_build && rm -rf ./node_build/app && mv build node_build/app",
    "build:src": "cross-env NODE_ENV=production babel src --out-dir node_build/src --copy-files",
    "build:ssr": "cross-env NODE_ENV=production babel ssr --out-dir node_build/ssr --copy-files",
    "build:node": "yarn build:src && yarn build:ssr",
    "----- // CLEANING SCRIPTS // -----": "",
    "clean:npm": "rm -rf ./node_modules",
    "clean": "rm -rf node_build && rm -rf build",
    "reset": "yarn clean && yarn clean:npm",
    "----- // STORYBOOK SCRIPTS // -----": "",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "babel": {
    "presets": [
      "./babel-preset.js"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@fontsource/manrope": "^4.5.1",
    "@loadable/component": "^5.15.0",
    "@material-ui/core": "^4.12.3",
    "@react-spring/web": "^9.4.2",
    "@vime/core": "^5.3.1",
    "@vime/react": "^5.3.1",
    "aws-sdk": "^2.1167.0",
    "body-parser": "^1.19.0",
    "clsx": "^1.1.1",
    "cookie-parser": "^1.4.5",
    "dayjs": "^1.10.7",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.7.2",
    "graphql-type-json": "^0.3.2",
    "history": "^4.9.0",
    "http-proxy-middleware": "^2.0.1",
    "isomorphic-fetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "jwtin": "^0.0.2",
    "millisecond": "^0.1.2",
    "node-env-file": "^0.1.8",
    "nodemailer": "^6.7.1",
    "pg": "^8.7.1",
    "pg-pubsub": "^0.6.1",
    "react": "^17.0.2",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-player": "^2.10.1",
    "react-redux": "^7.2.6",
    "react-redux-feature": "^0.3.4",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.1",
    "redux": "^4.1.2",
    "redux-events-middleware": "^1.5.2",
    "redux-thunk": "^2.4.0",
    "sequelize": "^6.9.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.0",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.16.0",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/cli": "^6.4.13",
    "@storybook/node-logger": "^6.4.13",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.13",
    "babel-loader": "^8.2.3",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "concurrently": "^6.4.0",
    "cra-bundle-analyzer": "^0.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-storybook": "^0.5.6",
    "nodemon": "^2.0.15",
    "react-scripts": "^4.0.3",
    "redux-devtools-extension": "^2.13.9"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
